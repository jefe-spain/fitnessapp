{
  "expo": {
    "name": "fitness-app",
    "slug": "fitness-app",
    "version": "1.0.0",
    "scheme": "fitnessapp",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png",
      "postcss": {
        "config": true
      }
    },
    "newArchEnabled": true,
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fitnessapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fitnessapp"
    },
    "developmentClient": {
      "silentLaunch": {
        "ios": {
          "simulator": {
            "deviceType": "iPhone 15 Pro"
          }
        }
      }
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a020d529-10ab-4969-8976-8df51ce15cac"
      }
    }
  }
}
