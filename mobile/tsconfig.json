{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "jsx": "react-jsx",
    "paths": {
      "@app/*": ["app/*"],
      "@components/*": ["components/*"],
      "@store/*": ["store/*"],
      "@utilities/*": ["utilities/*"],
      "@i18n/*": ["i18n/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "nativewind-env.d.ts"]
}
